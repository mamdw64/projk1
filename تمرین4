age = 18 
if age < 10:
    print("کودک") 
elif age <= 18:
    print("نوجوان") 
else: print("بزرگسال")

nomre = 20 
if nomre < 10:
   print( ' Effort ')
elif nomre > 10:
     print( ' pass ') 
else: nomre <=18 
    print( ' nice ')


def factorial(n):
    if n == 0:
          return 1 
      else: 
return n * factorial(n - 1)  
   print(factorial(5))
