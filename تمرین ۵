import math

def fun1(x , y):
  return math.sqrt(x**2+y**2)

def degTOrad(degrees):
    radians = degrees*(math.pi/180)
    return radians

def fun2(x):
    xd = degTOrad(x)
    return math.sqrt(math.sin(xd)/x)

def fun3(x , a , w):
  return x-a*x/w

def fun4(firstStr , secondStr):
  return firstStr +' '+ secondStr

print (fun1(2,5))
print (fun2(60))
print (fun3(2,5,4))
print (fun4('ali' , 'gholi'))
