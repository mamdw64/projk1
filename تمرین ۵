import math

c = math.sqrt(64)
d = math.sqrt(80)
e = math.tan(75)
f = math.sin(60)

print('  √64  = {}\n  √80  = {}\n tan75 = {}\n sin60 = {}'.format(c,d,e,f))
  √64  = 8.0
  √80  = 8.94427190999916
